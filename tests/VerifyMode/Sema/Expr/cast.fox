// RUN: %fox-verify

func foo() {
  // Booleans upranking
  true as int;
  true as float;

  // Ints upranking
  1 as float;

  // Float downranking
  3.14 as int;
  3.14 as bool;

  // Int downranking
  3 as bool;

  // Arrays
  [true, false, true] as [int];
  [true, false, true] as [float];

  // Inference
  [] as [int];
  [[]] as [[float]];
  [] as int; // expect-error: cannot cast expression of type '[any]' to type 'int'
             // expect-error@-1: failed to infer the type of this expression

  // Redundant cast 
  1 as int; 
  // expect-warning@-1: useless cast to 'int': expression is already of type 'int'
  3.14 as float; 
  // expect-warning@-1: useless cast to 'float': expression is already of type 'float'
  true as bool; 
  // expect-warning@-1: useless cast to 'bool': expression is already of type 'bool'
  [[3.14],[]] as [[float]]; 
  // expect-warning@-1: useless cast to '[[float]]': expression is already of type '[[float]]'
}