// RUN: %fox-run

func foo() {
  fib(16);
}

func fib(n: int) {
  var prevPrev : int;
  var prev : int;
  var cur : int = 1;
  var k : int = 1;
  while(k < n) {
    prevPrev = prev;
    prev = cur;
    cur = prevPrev + prev;
    k = k+1;
    printInt(cur);
    printChar('\n');
  }
}

